    <modelVersion>4.0.0</modelVersion>

    <!--配置open feign模块坐标-->
    <artifactId>feign-server</artifactId>
    <groupId>com.newer.feign</groupId>
    <version>1.0-SNAPSHOT</version>


    <dependencies>
        <!--导入shop-common模块-->
